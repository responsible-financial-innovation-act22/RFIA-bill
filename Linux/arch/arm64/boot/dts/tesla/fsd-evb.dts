/dts-v1/;
#include "fsd.dtsi"

/ {
	model = "Tesla Full Self-Driving (FSD) Evaluation board";
	compatible = "tesla,fsd-evb", "tesla,fsd";

	aliases {
		serial0 = &serial_0;
		serial1 = &serial_1;
	};

	chosen {
		stdout-path = &serial_0;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x2 0x00000000>;
	};
};

&fin_pll {
	clock-frequency = <24000000>;
};

&serial_0 {
	status = "okay";
};
